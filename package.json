{
  "name": "@habx/lib-client-features",
  "version": "0.0.4",
  "description": "Wrapper for using feature flags",
  "main": "./dist/index.js",
  "types": "./dist/typings/index.d.ts",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && bili && npm run build:types",
    "build:dev": "npm run clean && tsc",
    "build:watch": "npm run clean && tsc --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "postbuild": "node ./scripts/postbuild.js",
    "publish": "cd dist && npm publish && cd ..",
    "lint": "eslint \"src/**\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/habx/lib-client-features.git"
  },
  "author": "Jean Dessane",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/habx/lib-client-features/issues"
  },
  "homepage": "https://github.com/habx/lib-client-features#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "react": "^16.8.1",
    "react-dom": "^16.8.1"
  },
  "devDependencies": {
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "babel-eslint": "^10.0.1",
    "bili": "^4.7.4",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-config-react-app": "^4.0.0",
    "eslint-plugin-flowtype": "^3.7.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-import-helpers": "^0.1.4",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "husky": "^2.2.0",
    "lint-staged": "^8.1.6",
    "prettier": "^1.17.0",
    "prettier-eslint": "^9.0.0",
    "prettier-eslint-cli": "^4.7.1",
    "rimraf": "^2.6.3",
    "rollup-plugin-multi-entry": "^2.1.0",
    "rollup-plugin-typescript2": "^0.21.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.3.3"
  }
}
